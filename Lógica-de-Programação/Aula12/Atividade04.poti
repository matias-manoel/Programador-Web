var n := vetor[100]
var num := 1
var indice := -1
enquanto num <> 0 faça
	num := leia_inteiro
	se num <> 0 então
		se indice < 100 então
			indice := indice + 1
			n[indice] := num
		fim
	fim
fim
escreva "Conteúdo do vetor:"
para i de 0 até indice faça
	imprima "{n[i]}, "
fim
escreva
para i de 0 até indice faça
	se i mod 2 == 0 então
		se n[i] mod n[i + 1] == 0 então
			escreva "{n[i]} é divisível por {n[i + 1]}"
		senão
			escreva "{n[i]} não é divisível por {n[i + 1]}"
		fim
	fim
fim