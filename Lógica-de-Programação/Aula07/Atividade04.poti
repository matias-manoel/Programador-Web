var n := leia_inteiro
var soma := 0
var contaDivisor := 0
var cont := 1
var divisor := 1
enquanto cont <= n faça
    enquanto divisor <= cont faça
        se cont mod divisor == 0 então
            contaDivisor := contaDivisor + 1
        fim
        divisor := divisor + 1
    fim
    se contaDivisor == 2 então
    	soma := soma + cont
    fim
    contaDivisor := 0
    divisor := 1
    cont := cont + 1
fim
imprima soma