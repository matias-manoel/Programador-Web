var n := leia_inteiro
var contaDivisor := 0
para i de 2 até n faça
	para j de 2 até i - 1 faça
    	se i mod j == 0 então
        	contaDivisor := contaDivisor + 1
        fim
    fim
    se contaDivisor == 0 então
    	escreva "{i} primo"
    senão
    	escreva "{i} composto"
        para k de 2 até i - 1 faça
        	se i mod k == 0 então
        		escreva k
            fim
        fim
    fim
    contaDivisor := 0
fim